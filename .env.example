# Copy to .env and fill with your Twilio credentials
TWILIO_ACCOUNT_SID=ACxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
TWILIO_AUTH_TOKEN=your_auth_token_here
# Must be a WhatsApp-enabled Twilio number, prefixed with whatsapp:
TWILIO_WHATSAPP_FROM=whatsapp:+17343367101

# Warelay Prediction Market (Enabled by default)
ENABLE_PREDICTIONS=true
PREDICTION_MARKETS=SOL/USD,BTC/USD
PREDICTION_INTERVAL=3600000
MIN_PREDICTION_CONFIDENCE=0.6

# Polymarket Predictions with Claude
ENABLE_POLYMARKET=true
POLYMARKET_INTERVAL=7200000
POLYMARKET_CATEGORIES=crypto,politics,sports,science
POLYMARKET_MIN_VOLUME=10000
POLYMARKET_MAX_MARKETS=5
POLYMARKET_SHARE_TOP=3

# Warelay Trading (Optional - only if you want autonomous trading)
ENABLE_TRADING=false
SOLANA_RPC_ENDPOINT=https://api.mainnet-beta.solana.com
SOLANA_PRIVATE_KEY=your_base58_private_key_here
MAX_TRADE_SIZE=0.1
MAX_PORTFOLIO_RISK=20
STOP_LOSS_PERCENT=15
TAKE_PROFIT_PERCENT=50
MIN_LIQUIDITY=10000
TRADING_CHECK_INTERVAL=15
MAX_TRADES_PER_DAY=10
SHARE_TRADES_PUBLICLY=false
TRADING_PAIRS=
